# Токен Telegram бота
BOT_TOKEN=your_bot_token_here

# Настройки PostgreSQL для Supabase
# Получите эти данные в панели Supabase: Project Settings -> Database
# Connection string можно найти в разделе Connection string -> URI или Connection pooling
# Пример хоста: db.xxxxxxxxxxxxx.supabase.co
DB_HOST=db.xxxxxxxxxxxxx.supabase.co
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=your_supabase_password_here

# Настройки логирования
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log

